{
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "git.ignoreLimitWarning": true,
  "editor.formatOnSave": true,
  "search.exclude": {
    "**/.git": true,
    "**/node_modules": true,
    "**/dist": true,
    "**/lib": true
  },
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
    "**/node_modules": true
  },
  "cSpell.words": [
    "cand",
    "canonicalize",
    "chainmail",
    "chall",
    "CHARFONT",
    "chestplate",
    "Gamemode",
    "gametest",
    "gametests",
    "lerp",
    "minecart",
    "SCORESHEET",
    "shulker",
    "tock",
    "Tocks",
    "xtralarge",
    "zoglin"
  ],
  "editor.tabSize": 2,
  "eslint.experimental.useFlatConfig": true
}
